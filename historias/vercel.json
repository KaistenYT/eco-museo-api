{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/health",
            "dest": "/health"
        },
        {
            "src": "/actors/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/authors/(.*)",
            "dest": "/authors/$1"
        },
        {
            "src": "/histories/(.*)",
            "dest": "/histories/$1"
        },
        {
            "src": "/participations/(.*)",
            "dest": "/participations/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}